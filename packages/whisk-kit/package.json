{
  "name": "@paperclip-labs/whisk-kit",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./format": {
      "import": "./dist/format/index.js",
      "require": "./dist/format/index.js"
    },
    "./identity/core": {
      "default": "./dist/identity/core/index.js"
    },
    "./identity": {
      "default": "./dist/identity/index.js"
    },
    "./ui": {
      "default": "./dist/ui/index.js"
    }
  },
  "scripts": {
    "stylegen": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "stylegen-watch": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "build": "pnpm stylegen && tsc && tsc-alias",
    "dev": "concurrently \"pnpm stylegen-watch\" \"pnpm tsc -w\" \"pnpm tsc-alias -w\""
  },
  "keywords": [],
  "author": "Paperclip Labs",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "concurrently": "^9.0.1",
    "jest": "^29.7.0",
    "next": "^14.2.15",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.59.15",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "viem": "2.x",
    "wagmi": "^2.9.0x"
  },
  "dependencies": {
    "@paperclip-labs/whisk": "workspace:*",
    "@neynar/nodejs-sdk": "^1.62.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "12.0.0-alpha.1",
    "hono": "^4.6.12",
    "lucide-react": "^0.453.0",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.47",
    "server-only": "^0.0.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tsc-alias": "^1.8.10",
    "zod": "^3.23.8"
  }
}
