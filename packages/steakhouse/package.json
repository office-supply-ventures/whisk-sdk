{
  "name": "@whisk/steakhouse",
  "version": "0.0.2",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "type": "module",
  "main": "./dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "src": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react": {
      "src": "./src/react/index.ts",
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./metadata": {
      "src": "./src/metadata/index.ts",
      "types": "./dist/metadata/index.d.ts",
      "default": "./dist/metadata/index.js"
    }
  },
  "dependencies": {
    "@urql/core": "catalog:",
    "@whisk/client": "workspace:*",
    "@whisk/graphql": "workspace:*",
    "gql.tada": "catalog:"
  },
  "peerDependencies": {
    "graphql": ">=16",
    "react": ">=18",
    "react-dom": ">=18",
    "@tanstack/react-query": ">=5"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "@tanstack/react-query": "catalog:"
  }
}
