{
  "name": "@paperclip-labs/whisk-sdk",
  "version": "0.0.10",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./identity/core": {
      "default": "./dist/identity/core/index.js"
    },
    "./identity": {
      "default": "./dist/identity/index.js"
    }
  },
  "scripts": {
    "stylegen": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "stylegen-watch": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "build": "pnpm stylegen && tsc && tsc-alias",
    "dev": "concurrently \"pnpm stylegen-watch\" \"pnpm tsc -w\" \"pnpm tsc-alias -w\""
  },
  "keywords": [],
  "author": "Paperclip Labs",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18",
    "concurrently": "^9.0.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "dependencies": {
    "@tanstack/react-query": ">=5",
    "@paperclip-labs/whisk-client": "^0.0.1",
    "clsx": "^2.1.1",
    "framer-motion": "12.0.0-alpha.1",
    "hono": "^4.6.12",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tsc-alias": "^1.8.10",
    "viem": "2.x"
  }
}
