{
  "name": "@whisk/client",
  "version": "0.0.17",
  "repository": {
    "type": "git",
    "url": "https://github.com/office-supply-ventures/whisk-sdk.git",
    "directory": "packages/client"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check:types": "tsgo --noEmit"
  },
  "type": "module",
  "main": "./dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
