      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@atmina/urql-custom-scalars-exchange": "1.2.0",
    "@urql/core": "catalog:",
    "@whisk/graphql": "workspace:*",
    "gql.tada": "catalog:"
  },
  "peerDependencies": {
    "graphql": ">=16"
