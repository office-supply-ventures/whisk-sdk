{
  "name": "whisk-sdk",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter=site...",
    "dev:next": "turbo run dev --filter=next-app...",
    "test": "vitest",
    "changeset:publish": "pnpm build && changeset publish",
    "changeset:version": "changeset version",
    "check": "biome check --fix --unsafe",
    "check:types": "tsc"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.5.1",
    "dotenv": "^17.2.3",
    "tsup": "^8.5.1",
    "turbo": "^2.6.1",
    "typescript": "5.9.2",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.23.0"
}
