{
  "name": "whisk-sdk",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf packages/*/{dist,.turbo} examples/*/{dist,.turbo,.next} site/{dist,.turbo} .turbo node_modules/.cache",
    "build": "turbo build",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter=site...",
    "dev:next": "turbo run dev --filter=next-app...",
    "test": "vitest",
    "changeset:publish": "pnpm build && changeset publish",
    "changeset:version": "changeset version",
    "check": "biome check --fix --unsafe",
    "check:graphql": "pnpm --filter @whisk/graphql check:graphql",
    "check:types": "turbo check:types && tsgo --noEmit",
    "generate:vaults": "pnpm --filter @whisk/steakhouse generate:vaults",
    "schema:sync": "pnpm --filter @whisk/graphql schema:sync",
    "schema:sync:local": "pnpm --filter @whisk/graphql schema:sync:local"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.5.1",
    "@typescript/native-preview": "7.0.0-dev.20260205.1",
    "dotenv": "^17.2.3",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "turbo": "^2.6.1",
    "typescript": "5.9.2",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.23.0"
}
