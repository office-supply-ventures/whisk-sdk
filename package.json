{
  "name": "whisk-sdk",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm --filter='./packages/*' build",
    "dev": "pnpm --filter='./packages/*' dev",
    "dev:docs": "pnpm run --filter='./site' dev",
    "test": "vitest",
    "changeset:publish": "pnpm --filter './packages/*' exec zile publish:prepare && changeset publish && pnpm --filter './packages/*' exec zile publish:post",
    "changeset:version": "changeset version",
    "check": "biome check --fix --unsafe",
    "check:types": "tsc"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.5.1",
    "typescript": "5.9.2",
    "vitest": "^4.0.10",
    "zile": "^0.0.13"
  },
  "packageManager": "pnpm@10.23.0"
}
